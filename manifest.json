{
  "manifest_version": 3,
  "name": "MyUrlAlarm",
  "version": "1.0",
  "description": "Alarm that won't stop unless you click on a custom URL",
  "permissions": [
    "alarms",
    "activeTab",
    "storage" 
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}